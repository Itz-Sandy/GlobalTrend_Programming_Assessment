def transpose_matrix(matrix):
    """
    Function to compute the transpose of a given matrix.
    
    Parameters:
    matrix (list of list): The input matrix represented as a 2D list.
    
    Returns:
    list of list: The transpose of the input matrix.
    """
    # Check if the matrix is empty
    if not matrix:
        return []
    
    # Dimensions of the matrix
    rows = len(matrix)
    cols = len(matrix[0])
    
    # Initialize a new matrix for the transpose
    transpose = [[0 for _ in range(rows)] for _ in range(cols)]
    
    # Compute the transpose
    for i in range(rows):
        for j in range(cols):
            transpose[j][i] = matrix[i][j]
    
    return transpose

# Example usage:
matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

transposed_matrix = transpose_matrix(matrix)
for row in transposed_matrix:
    print(row)
